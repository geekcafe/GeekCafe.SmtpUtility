<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>net6.0</TargetFramework>
    <Version>1.6.0</Version>
    <Authors>Eric Wilson</Authors>
    <Company>Geek Cafe LLC</Company>
    <PackageId>GeekCafe.SmtpUtility</PackageId>
    <PackageIcon>GeekCafe.png</PackageIcon>
    <Description>Smtp Utility Service for consumption in .net core projects</Description>
    <PackageLicenseExpression>MIT</PackageLicenseExpression>
    <PackageProjectUrl>http://github.com/geekcafe/GeekCafe.SmtpUtility</PackageProjectUrl>
  </PropertyGroup>

  <ItemGroup>
    <None Remove="GeekCafe.SmtpUtility.Interfaces" />
    <None Remove="Microsoft.Extensions.Logging.Abstractions" />
  </ItemGroup>
  <ItemGroup>
       <None Include="images\GeekCafe.png" Pack="true" PackagePath="\" />
  </ItemGroup>
  
  <ItemGroup>
    <PackageReference Include="GeekCafe.SmtpUtility.Interfaces" Version="1.6.0" />
    <PackageReference Include="Microsoft.Extensions.Logging.Abstractions" Version="5.0.0" />
  </ItemGroup>
</Project>
